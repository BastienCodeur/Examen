{% extends 'base.html.twig' %}

{% block title %}Support Ticket!{% endblock %}

{% block body %}
    <table class="table">
        <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nom</th>
            <th scope="col">Prix</th>
            <th scope="col">description</th>

        </tr>
        </thead>    {% for abonnement in abonnements %}


            <tr>
                <td>{{ abonnement.id }}</td>
                <td>{{ abonnement.nom }}</td>
                <td>{{ abonnement.prix }}</td>
                <td>{{ abonnement.description }}</td>





            </tr>


        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}